    "compilerOptions": {
        "composite": true,
        "lib": ["ESNext"],
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "resolveJsonModule": true,
        "noEmit": true,
        "isolatedModules": true,
        "strict": true,
        "skipLibCheck": true
